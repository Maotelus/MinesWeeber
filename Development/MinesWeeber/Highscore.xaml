<Window x:Class="MinesWeeber.Highscore"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MinesWeeber"
        mc:Ignorable="d"
        Title="Highscore" Height="300" Width="382.41" Loaded="Window_Loaded">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <ListBox x:Name="libx_Scores" HorizontalAlignment="Left" Height="216" Margin="56,26,0,0" VerticalAlignment="Top" Width="249" SelectionChanged="libx_Scores_SelectionChanged"/>
        <Label x:Name="lbl_Highscore" Content="Highscore" HorizontalAlignment="Left" Margin="144,0,0,0" VerticalAlignment="Top" Width="75" Height="26"/>
        <Button x:Name="btn_Back" Content="Back" HorizontalAlignment="Left" Margin="144,247,0,0" VerticalAlignment="Top" Width="75" Click="btn_Back_Click" Height="20"/>

    </Grid>
</Window>